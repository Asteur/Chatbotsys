{
  "chainer": {
    "in": [
      "question_raw"
    ],
    "out": [
      "answers"
    ],
    "pipe": [
      {
        "config_path": "../deeppavlov/configs/odqa/ru_ranker_prod.json",
        "in": [
          "question_raw"
        ],
        "out": [
          "doc_ids"
        ]
      },
      {
        "name": "wiki_sqlite_vocab",
        "in": [
          "doc_ids"
        ],
        "out": [
          "contexts"
        ],
        "data_dir": "odqa",
        "join_docs": false,
        "shuffle": false,
        "load_path": "http://lnsigo.mipt.ru/export/datasets/wikipedia/ruwiki.db"
      },
      {
        "name": "squad_wrapper",
        "config_path1": "../deeppavlov/configs/squad/squad_ru.json",
        "in": ["question_raw", "contexts"],
        "out": ["answers"]
      }
    ]
  },
  "metadata": {
    "labels": {
      "server_utils": "ODQA"
    },
    "download": [
      "http://lnsigo.mipt.ru/export/deeppavlov_data/ru_odqa.tar.gz",
      "http://lnsigo.mipt.ru/export/deeppavlov_data/squad_model_ru.tar.gz"
    ]
  }
}